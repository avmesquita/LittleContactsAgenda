@model IEnumerable<LittleAgenda.Entity.Telephone>

@if (Model.Count() > 0)
{
	<h5>
		Telephones
	</h5>

	<table class="table-condensed">
		<tr>
			<th></th>
		</tr>
		@foreach (var item in Model)
		{
			<tr>
				<td>
					@Html.DisplayFor(modelItem => item.TelephoneContent)
				</td>
			</tr>
		}
	</table>
}
