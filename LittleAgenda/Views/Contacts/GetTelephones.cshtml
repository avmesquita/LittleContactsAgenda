@model IEnumerable<LittleAgenda.Entity.Telephone>

<h4>
	Telephones
</h4>

@if (Model.Count() > 0)
{
	<table class="table">
		<tr>
			<th></th>
		</tr>
		@foreach (var item in Model)
		{
			<tr>
				<td>
					@Html.DisplayFor(modelItem => item.TelephoneContent)
				</td>
			</tr>
		}
	</table>
}
else
{
	<p style="text-align:center;">
		No Telephones found.
	</p>

}
