@model IEnumerable<LittleAgenda.Entity.Address>
@if (Model.Count() > 0)
{
	<h5>
		Addresses
	</h5>
	<table class="table-condensed">
		<tr>
			<th></th>
		</tr>
		@foreach (var item in Model)
		{
			<tr>
				<td>
					@Html.DisplayFor(modelItem => item.AddressContent)
				</td>
			</tr>
		}
	</table>
}
